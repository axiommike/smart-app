<header role="banner" data-current-step="{{currentStep}}">
	{{wizard-steps steps=steps currentStepNumber=currentStep}}
	<aside class="agent-callout">
		<h1 class="agent-callout-name">Sample Agent</h1>
		<a class="agent-callout-phone" href="tel:15555555555">{{accessible-icon icon="phone" title="Call me now"}} +1 555-555-5555</a>
	</aside>
	{{#liquid-if currentStep}}
		<button {{action "sendIncomplete"}} class="header-action">Submit As-Is</button>
	{{/liquid-if}}
</header>

<datalist id="lenders">
	{{#each lender in lenders}}
		<option {{bind-attr value=lender}}></option>
	{{/each}}
</datalist>
<main role="main">
	{{liquid-outlet}}
</main>

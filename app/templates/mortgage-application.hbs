<header role="banner" data-current-step="{{currentStep}}">
	{{wizard-steps steps=steps currentStepNumber=currentStep}}
	<button {{action "sendIncomplete"}} class="header-action">Call Me {{accessible-icon icon="phone" title="Call me for immediate assistance"}}</button>
</header>

<datalist id="lenders">
	{{#each lender in lenders}}
		<option {{bind-attr value=lender}}></option>
	{{/each}}
</datalist>
<main role="main">
	{{liquid-outlet}}
</main>
